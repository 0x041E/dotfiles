! disable the fucking bell
URxvt.insecure: false
! URxvt.transparent: true
URxvt.depth: 32
URxvt.termName: rxvt-256color
!URxvt.background: rgba:0000/0000/0200/c800
! URxvt.background: rgba:0000/0000/0000/0000
! URxvt.shading: 0 to 99 darkens, 101 to 200 lightens
! URxvt.shading: 15
URxvt.iconFile: /usr/share/icons/Arc/apps/symbolic/utilities-terminal-symbolic.svg
URxvt.font: xft:ProFont:Regular:size=8,antialias=true,\
            xft:Kochi Gothic:antialias=false,\
            xft:NanumSquare:Regular

!            xft:Baekmuk Gulim:antialias=true,
!URxvt.boldfont: xft:Hack:Bold:pixelsize=7,antialias=true,\
!            xft:Kochi Gothic:antialias=false,\
!            xft:NanumGothic:Bold

!  xft:Baekmuk Gulim:Regular:size=7,antialias=true
! URxvt.font: xft:Droid Sans Mono:Regular:size=8,\
! xft:Baekmuk Gulim:Regular:size=8
! URxvt.letterSpace: -1
! URxvt.letterSpace: 0
! URxvt.lineSpace: -1
URxvt.allow_bold: True
! URxvt.foreground: #D3D3D3
! URxvt.foreground: #AA00FF
! URxvt.highlightTextColor: #000000
! URxvt.highlightColor: #AA00FF
! URxvt.cursorBlink: True
! URxvt.background: rgba:0000/0000/0000/aaaa
! - catch ugly URLs
! URxvt.cutchars: `()'*<>[]{|}"
! URxvt.cutchars: `"'%&()*,;<>@[]{|}"
URxvt.perl-ext-common: default,matcher
!,clipboard
URxvt.perl-ext: xim-onthespot
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.inputMethod: fcitx
! URxvt.inputMethod: ibus
! URxvt.preeditType: OverTheSpot
! URxvt.preeditType: OnTheSpot,OverTheSpot,None

*.inputMethod: fcitx
! *.inputMethod: ibus
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

URxvt.saveLines: 10000
URxvt.mouseWheelScrollPage: false
URxvt.scrollBar: True
URxvt.scrollBar_right: 1
URxvt.secondaryScroll: True
URxvt.scrollTtyOutput: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true
! URrxvt.scrollstyle: xterm
URxvt.scrollstyle: plain
URxvt.tabbedex.autohide: yes
URxvt.tabbedex.tabbar-fg: 2
URxvt.tabbedex.tabbar-bg: 0
URxvt.tabbedex.tab-fg: 10
URxvt.tabbedex.tab-bg: 0
URxvt.tabbedex.title: yes
URxvt.tabbedex.new-button: no

URxvt.modifier: control
xterm.borderWidth: 0
xterm.utf8: 2
!xterm.font: xft:Hack:Regular:size=7\
! xterm.font: xft:Misc Ohsnap:size=7\
!   xft:NanumSquare:Regular:size=7
xterm.font: xft:Hack:Regular:size=7,antialias=true,\
            xft:Kochi Gothic:antialias=false,\
            xft:Baekmuk Gulim

! Xft.autohint: 0
! Xft.antialias: 1
! Xft.hinting: 1
! ! Xft.dpi: 106
! Xft.lcdfilter: lcddefault
! Xft.hintstyle: hintslight
! Xft.rgba: rgb

! Xcursor.theme: Eclipse
! Xcursor.theme: Sixus
! Xcursor.theme: CG
! Xcursor.theme: Cyclone
! Xcursor.theme: redglass
! Xcursor.theme: Simple-and-Soft
! Xcursor.theme: Red-Dot
! Xcursor.theme: NeoAlien-dark
! Xcursor.theme: Oxygen Zion
Xcursor.theme: ATER-yellow
! Xcursor.theme: Neutral
! Xcursor.theme: deepin
! Xcursor.theme: Pulse-Glass
! Xcursor.theme: handhelds
! Xcursor.theme: Numix

! Xcursor.size: 16

! FONTS
! urxvt*font: xft:gohufont:size=7:antialias=true,\
! xft:Baekmuk Gulim:Regular:size=7:antialias=true
! urxvt*boldFont: xft:gohufont:size=7:antialias=true

!#include ".Xresources.d/theme1"
#include ".Xresources.d/green"
!#include ".Xresources.d/nature"
!#include ".Xresources.d/blackwhite"
!#include ".Xresources.d/gotham"
